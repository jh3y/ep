colorPalette = {
  green  : #2ECC71
  blue   : #3498DB
  purple : #9B59B6
  yellow : #F1C40F
  orange : #E67E22
  red    : #E74C3C
}

for key, value in colorPalette
  .progrecss[data-progrecss].{key},
  .progrecss[data-progrecss-mock].{key},
  .progrecss[data-progrecss-timer].{key}
    &:after
      background-color value
